{
    "minimum-stability": "dev",
    "require": {
        "doctrine/common": ">=2.1",
        "maglnet/magl-markdown": "~1.1",
        "phly/mustache": "1.*",
        "phly/phly-blog": "dev-master",
        "phly/phly-contact": "dev-master",
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.2.*",
        "zendframework/zendservice-recaptcha": "2.0.*"
    },
    "autoload": {
